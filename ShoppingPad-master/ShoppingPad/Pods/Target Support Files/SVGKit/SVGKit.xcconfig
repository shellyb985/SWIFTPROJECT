CLANG_CXX_LANGUAGE_STANDARD = gnu++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/SVGKit" "${PODS_ROOT}/Headers/Public" $(SDKROOT)/usr/include/libxml2
OTHER_LDFLAGS = -l"xml2" -framework "CoreText" -framework "QuartzCore"
PODS_ROOT = ${SRCROOT}
SKIP_INSTALL = YES